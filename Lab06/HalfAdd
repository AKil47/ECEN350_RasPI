    %store/vec4 v0x5555898430_0, 0, 8;
    %delay 90000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55558980a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555898190_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x5555898260_0;
    %load/vec4 v0x5555898360_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x5555897d10_0, 0, 2;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x5555897df0_0, 0, 2;
    %pushi/vec4 3156784, 0, 121;
    %store/vec4 v0x5555897fc0_0, 0, 121;
    %load/vec4 v0x5555898430_0;
    %store/vec4 v0x5555897ed0_0, 0, 8;
    %fork TD_HalfAddTest_v.passTest, S_0x5555897b30;
    %join;
    %load/vec4 v0x5555897ed0_0;
    %store/vec4 v0x5555898430_0, 0, 8;
