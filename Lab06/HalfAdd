S_0x5555841d20 .scope module, "HalfAddTest_v" "HalfAddTest_v" 2 26;
 .timescale -9 -12;
v0x55558980a0_0 .var "A", 0 0;
v0x5555898190_0 .var "B", 0 0;
v0x5555898260_0 .net "Cout", 0 0, L_0x5555898630;  1 drivers
v0x5555898360_0 .net "Sum", 0 0, L_0x5555898850;  1 drivers
v0x5555898430_0 .var "passed", 7 0;
S_0x5555841eb0 .scope task, "allPassed" "allPassed" 2 37, 2 37 0, S_0x5555841d20;
 .timescale -9 -12;
v0x5555842090_0 .var "numTests", 7 0;
v0x5555897230_0 .var "passed", 7 0;
TD_HalfAddTest_v.allPassed ;
    %load/vec4 v0x5555897230_0;
    %load/vec4 v0x5555842090_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 41 "$display", "All tests passed" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 42 "$display", "Some tests failed" {0 0 0};
T_0.1 ;
    %end;
S_0x5555897310 .scope module, "dut" "HalfAdd" 2 55, 3 1 0, S_0x5555841d20;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 1 "Cout";
    .port_info 1 /OUTPUT 1 "Sum";
    .port_info 2 /INPUT 1 "A";
    .port_info 3 /INPUT 1 "B";
L_0x5555898520 .functor NAND 1, v0x55558980a0_0, v0x5555898190_0, C4<1>, C4<1>;
L_0x5555898630 .functor NAND 1, L_0x5555898520, L_0x5555898520, C4<1>, C4<1>;
L_0x5555898740 .functor NAND 1, L_0x5555898520, v0x55558980a0_0, C4<1>, C4<1>;
L_0x55558987b0 .functor NAND 1, L_0x5555898520, v0x5555898190_0, C4<1>, C4<1>;
L_0x5555898850 .functor NAND 1, L_0x5555898740, L_0x55558987b0, C4<1>, C4<1>;
v0x5555897510_0 .net "A", 0 0, v0x55558980a0_0;  1 drivers
v0x55558975d0_0 .net "B", 0 0, v0x5555898190_0;  1 drivers
v0x5555897690_0 .net "Cout", 0 0, L_0x5555898630;  alias, 1 drivers
v0x5555897760_0 .net "Sum", 0 0, L_0x5555898850;  alias, 1 drivers
v0x5555897820_0 .net "t1", 0 0, L_0x5555898520;  1 drivers
v0x5555897930_0 .net "t2", 0 0, L_0x5555898740;  1 drivers
v0x55558979f0_0 .net "t3", 0 0, L_0x55558987b0;  1 drivers
S_0x5555897b30 .scope task, "passTest" "passTest" 2 28, 2 28 0, S_0x5555841d20;
 .timescale -9 -12;
v0x5555897d10_0 .var "actualOut", 1 0;
v0x5555897df0_0 .var "expectedOut", 1 0;
v0x5555897ed0_0 .var "passed", 7 0;
v0x5555897fc0_0 .var "testType", 120 0;
TD_HalfAddTest_v.passTest ;
    %load/vec4 v0x5555897d10_0;
    %load/vec4 v0x5555897df0_0;
    %cmp/e;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 33 "$display", "%s passed", v0x5555897fc0_0 {0 0 0};
    %load/vec4 v0x5555897ed0_0;
    %addi 1, 0, 8;
    %store/vec4 v0x5555897ed0_0, 0, 8;
    %jmp T_1.3;
T_1.2 ;
    %vpi_call 2 34 "$display", "%s failed: %d should be %d", v0x5555897fc0_0, v0x5555897d10_0, v0x5555897df0_0 {0 0 0};
T_1.3 ;
    %end;
    .scope S_0x5555841d20;
T_2 ;
    %vpi_call 2 62 "$dumpfile", "HalfAdd.vcd" {0 0 0};
    %vpi_call 2 62 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5555841d20;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55558980a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5555898190_0, 0, 1;
